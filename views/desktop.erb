            //     });


                //click handler for sidebar links
                $(".sidebar a:not(.no-change), .logo").click(function(event){
                    
                    //for hover colors
                    $(".sidebar a").removeClass("chosen");
                	$(event.currentTarget).toggleClass("chosen");

                    var page_divs = ['#building_html', '#rez_html', '#kitchens_html', '#bathrooms_html', '#penthouses_html', '#amenities_html', '#better_living_html', '#open_air_html', '#neigh_html', '#map_html', '#avail_html', '#facts_html', '#team_html', '#press_html', '#contact_html', '#register_html', '#legal_html']

                    // store and pull correct html contents from the hidden divs
                    var empty_div;
                    $.each(page_divs, function(index, value){ 
                          if ($(value).is(':empty')){
                             empty_div = $(value);
                          } 
                    });
                    var current_contents = $(".content-area").contents();
                    empty_div.append(current_contents);
                    var id_to_render = $(event.currentTarget).attr("renders-this");
                    var new_contents = $(id_to_render).contents();    
                    $(".content-area").empty().append(new_contents);

                    //resize everything div to fit contents (why doesnt this shrink?)
                    //setTimeout(function(){ $(".everything").height($(".content-area").height()+40); },100);


                    //collapse on click for map accordion
                    $(".map-accordion").on('click', 'a.close-accordion-hack', function(event){
                        if($(event.currentTarget).parent().parent().hasClass('active')){
                            window.setTimeout(function(){
                            $(event.currentTarget).parent().parent().removeClass('active')
                         }, 100);
                        }
                    });  
                });

                $(".content-area > .controls").on('click','img.left_arrow', function(event){
                    $(".content-area > .orbit-container > .orbit-prev").trigger( "click" );
                    $(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > img.cent.white-border"));

                });
                $(".content-area > .controls").on('click','img.right_arrow', function(event){
                    $(".content-area > .orbit-container > .orbit-next").trigger( "click" );
                    $(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > img.cent.white-border"));

                });

                //$(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > .controls"));
                $(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > img.cent.white-border"));


                $('.sidebar a, .logo').on('click', function() {
                    setTimeout(function() {
                        if ($('.orbit-container ul').height() === 0) {
                            console.log("height is zero")
                            $(window).trigger('resize');
                            //$('.orbit-container ul').height(360);
                        }
                    }, 1);
                });

                $('.sidebar a').on('click', function(event){
                    document.location.hash = event.currentTarget.id;
                });


            });
