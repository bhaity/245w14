                    // $("#pan").animate({
                    //   bottom: "0"
                    // }, 8000, 'swing', function() {
                      
                    // });
                });


                // intizialize content area from building_html div and enable left/right arrows
                var new_contents = $('#building_html').contents();
                $(".content-area").append(new_contents);
                $(".content-area > .controls").on('click','img.left_arrow', function(event){
                  $(".content-area > .orbit-container > .orbit-prev").trigger( "click" );
                });
                $(".content-area > .controls").on('click','img.right_arrow', function(event){
                  $(".content-area > .orbit-container > .orbit-next").trigger( "click" );
                });  
               
                //move orbit bullets to after the slideshow arrows
                //$(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > .controls"));
                $(".content-area > .orbit-container > .orbit-bullets").insertAfter((".content-area > img.cent.white-border"));

               //resize orbit-container to fit slideshow contents
