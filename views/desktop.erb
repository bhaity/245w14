                 	    $("#rez_sub").slideUp("slow", function() {
                 	    	$(event.currentTarget).removeClass("opened");
                 	    });
                 	  }
                 	  else{
                      $("#rez_sub").slideDown("slow", function() {
                      	$(event.currentTarget).addClass("opened");
                      });
                 	  }
                });
                $("#amenities").click(function(event) {
                 	  if ($(event.currentTarget).hasClass("opened")){
                 	    $("#amen_sub").slideUp("slow", function() {
                 	    	$(event.currentTarget).removeClass("opened");
                 	    });
                 	  }
                 	  else{
                      $("#amen_sub").slideDown("slow", function() {
                      	$(event.currentTarget).addClass("opened");
                      });
                 	  }
                });

                $(".logo, #building-link").click(function(){
